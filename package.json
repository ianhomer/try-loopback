{
  "name": "try-loopback",
  "version": "1.0.0",
  "description": "try-loopback",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t try-loopback .",
    "docker:run": "docker run -p 3000:3000 -d try-loopback",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test",
    "update": "npx npm-check-updates -u && npm install --package-lock-only --no-audit"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^3.0.1",
    "@loopback/context": "^3.11.1",
    "@loopback/core": "^2.10.1",
    "@loopback/openapi-v3": "^4.0.1",
    "@loopback/repository": "^3.0.1",
    "@loopback/rest": "^7.0.1",
    "@loopback/rest-explorer": "^3.0.1",
    "@loopback/service-proxy": "^3.0.1",
    "loopback-connector-postgresql": "^5.1.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.4",
    "source-map-support": "^0.5.19",
    "@loopback/testlab": "^3.2.6",
    "@types/node": "^14.11.2",
    "@typescript-eslint/parser": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@loopback/eslint-config": "^10.0.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint-plugin-mocha": "^8.0.0",
    "minimist": ">=1.2.5",
    "typescript": "~4.0.3"
  }
}
